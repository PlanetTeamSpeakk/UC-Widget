			<listEntry value="0xAA00FF">@Strings.ColourPurple</listEntry>
			<listEntry value="0xFF00FF">@Strings.ColourPink</listEntry>
		</settingConfig>
	</setting>
	
	<setting propertyKey="@Properties.CustomTextColour" title="@Strings.CustomTextColourTitle" prompt="@Strings.CustomTextColourPrompt">
		<settingConfig type="alphaNumeric"/>
